{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "baseUrl": "./",
    "module": "esnext",
    "types": [],
    "paths": {
      "ngx-color/sketch": [ "../node_modules/ngx-color/sketch" ],
      "@supine/dy-form": [
        "../dist/dy-form/dy-form",
        "../dist/dy-form"
      ],
      "@supine/dy-form-zorro": [
        "../dist/dy-form-zorro/dy-form-zorro",
        "../dist/dy-form-zorro"
      ],
      "@supine/validator": [
        "../dist/validator/validator",
        "../dist/validator"
      ],
      "@supine/http": [
        "../dist/http/http",
        "../dist/http"
      ]
    }
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ],
  "angularCompilerOptions": {
    "annotateForClosureCompiler": true,
    "strictTemplates": true,
    "strictMetadataEmit": true,
    "strictInjectionParameters":true
  }
}
