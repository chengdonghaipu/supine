{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      false,
      "attribute",
      "jd",
      "camelCase"
    ],
    "component-selector": [
      false,
      "element",
      "jd",
      "kebab-case"
    ],
    "directive-class-suffix": false,
    "component-class-suffix": false,
    "variable-name": false,
    "typedef": false,
    "no-redundant-jsdoc": false,
    "no-unused-expression": false,
    "prefer-for-of": false,
    "forin": false
  }
}
